{
    "name": "qubiqx/qcommerce-ecommerce-core",
    "description": "Qcommerce Ecommerce Core package",
    "keywords": [
        "Qubiqx",
        "laravel",
        "qcommerce-ecommerce-core"
    ],
    "homepage": "https://github.com/qubiqx/qcommerce-ecommerce-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@qubiqx.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.15",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "daltcore/lara-pdf-merger": "^2.0",
        "hardevine/shoppingcart": "^3.2",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.5",
        "pestphp/pest": "^2.8",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Qubiqx\\QcommerceEcommerceCore\\": "src",
            "Qubiqx\\QcommerceEcommerceCore\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Qubiqx\\QcommerceEcommerceCore\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubiqx\\QcommerceEcommerceCore\\QcommerceEcommerceCoreServiceProvider"
            ],
            "aliases": {
                "QcommerceEcommerceCore": "Qubiqx\\QcommerceEcommerceCore\\Facades\\QcommerceEcommerceCore",
                "CurrencyHelper": "Qubiqx\\QcommerceEcommerceCore\\Classes\\CurrencyHelper",
                "EcommerceAccountHelper": "Qubiqx\\QcommerceEcommerceCore\\Classes\\EcommerceAccountHelper",
                "ShoppingCart": "Qubiqx\\QcommerceEcommerceCore\\Classes\\ShoppingCart",
                "ShippingZones": "Qubiqx\\QcommerceEcommerceCore\\Classes\\ShippingZones",
                "ProductCategories": "Qubiqx\\QcommerceEcommerceCore\\Classes\\ProductCategories",
                "Products": "Qubiqx\\QcommerceEcommerceCore\\Classes\\Products",
                "Product": "Qubiqx\\QcommerceEcommerceCore\\Models\\Product",
                "Orders": "Qubiqx\\QcommerceEcommerceCore\\Classes\\Orders",
                "SKUs": "Qubiqx\\QcommerceEcommerceCore\\Classes\\SKUs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
